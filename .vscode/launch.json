    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: COMAN AGENT",
            "type": "debugpy",
            "request": "launch",
            "module": "Agents.gym_coman"
        },
        {
            "name": "Python Debugger: COMAN CPG",
            "type": "debugpy",
            "request": "launch",
            "module": "Coman_CPG"
        },
        {
            "name": "Python Debugger: Talos CPG",
            "type": "debugpy",
            "request": "launch",
            "module": "Talos_CPG"
        },
    ]
